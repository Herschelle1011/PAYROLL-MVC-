<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Payroll_System</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Payroll_System.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

</head>


<body>
    <header class="main-nav">
        <navbar class="navbar">
            <img src="https://rocksontech.com/wp-content/uploads/2024/12/RS_LOGO_LANDSCAPE_WHITE.svg" />
            <P class="current-page">Dashboard</P>
            <div class="list-items">
                <i class="bi bi-search search"></i>
                <i class="bi bi-bell notification"></i>
                <img src="https://cdn-icons-png.flaticon.com/512/3135/3135823.png" class="user" />
                <p class="username">Renalie<i class="bi bi-chevron-down"></i></p>
                <partial name ="_LoginPartial" />
            </div>


        </navbar>


        <div class="side-bar">
            <div class="side-elements">

                <svg width="45" height="30" viewBox="0 0 42 29" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="logo-side">
                    <rect width="42" height="29" fill="url(#pattern0_201_64)" />
                    <defs>
                        <pattern id="pattern0_201_64" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_201_64" transform="scale(0.0119048 0.0172414)" />
                        </pattern>
                        <image id="image0_201_64" width="84" height="58" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>

                <div class="icons">
                    <hr class="small-line line">
                    
                    @* FOR DASHBOARD *@
                    <div class="hoverDashboard tooltip-icon" data-tooltip="Dashboard">
                        <a asp-controller="Admin" asp-action="Dashboard">
                            <i class="bi bi-grid-3x3-gap icon-sidebar"></i>
                        </a>
                    </div>

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Calendar">
                        <a asp-page="/Admin/calendar">
                            <i class="bi bi-calendar2 icon-sidebar"></i>
                        </a>
                    </div>

                    <div class="hoverDashboard tooltip-icon" data-tooltip="File Text">
                        <a asp-page="/Admin/filetext">
                            <i class="bi bi-file-text icon-sidebar"></i>
                        </a>
                    </div>

                    <hr class="small-line line">

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Boxes">
                        <a asp-page="/Admin/boxes">
                            <i class="bi bi-boxes icon-sidebar"></i>
                        </a>
                    </div>

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Bar Graph Report">
                        <a asp-page="/Admin/bargraph">
                            <i class="bi bi-file-bar-graph icon-sidebar"></i>
                        </a>
                    </div>

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Growth">
                        <a asp-page="/Admin/growth">
                            <i class="bi bi-graph-up-arrow icon-sidebar"></i>
                        </a>
                    </div>

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Organization Diagram">
                        <a asp-page="/Admin/diagram">
                            <i class="bi bi-diagram-3 icon-sidebar"></i>
                        </a>
                    </div>

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Lab Tools">
                        <a asp-page="/Admin/lab">
                            <i class="bi bi-beaker-fill icon-sidebar"></i>
                        </a>
                    </div>

                    <hr class="small-line line">

                    <div class="hoverDashboard tooltip-icon" data-tooltip="Settings">
                        <a asp-page="/Login">
                            <i class="bi bi-gear icon-sidebar settings"></i>
                        </a>
                    </div>

    </header>
    <br />
    <br />
    <br />
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)

    <footer class="footer-section">
        <div class="footer-elements">
            <img src="https://rocksontech.com/wp-content/uploads/2024/12/RS_LOGO_LANDSCAPE_WHITE.svg" alt="Rocksontech Logo" />

            <p><i class="bi bi-envelope-at-fill"></i> contact@rocksontech.com</p>
            <p>Copyright ©2025 Rocksontech. Designed by CoreConcepts.</p>
        </div>
    </footer>


</body>
</html>
